= form_for :withdrawal, url: admin_finance_withdrawal_transfer_path(resource), method: :put, html: { data: {confirm: "确认已经进行过转账了吗？" }} do |t|
  = hidden_field_tag 'go', 'transfer'
  = t.submit "确认转账", class: "btn btn-sm btn-success js-table-nolink"

= button_tag '转账失败', class: 'btn btn-sm btn-danger js-table-nolink', type: 'button', data: { toggle: 'modal', target: '#js-transferFailModal', id: "#{resource.id}", failmsg: "#{ resource.reason_message }", name: "#{resource.handyman.name}", total: "#{resource.total}", url: "#{admin_finance_withdrawal_transfer_path(resource)}" }

